<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quspacedragon.workflow.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.quspacedragon.workflow.entity.User">
        <result column="id" property="id"/>
        <result column="is_valid" property="isValid"/>
        <result column="op_time" property="opTime"/>
        <result column="last_ver" property="lastVer"/>
        <result column="status" property="status"/>
        <result column="type" property="type"/>
        <result column="attribute" property="attribute"/>
        <result column="create_time" property="createTime"/>
        <result column="name" property="name"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	id,
	is_valid AS isValid,
	op_time AS opTime,
	last_ver AS lastVer,
	status,
	type,
	attribute,
	create_time AS createTime,
        name
    </sql>

</mapper>
