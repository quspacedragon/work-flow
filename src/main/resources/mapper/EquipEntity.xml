<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quspacedragon.workflow.mapper.EquipEntityMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.quspacedragon.workflow.entity.EquipEntity">
	<result column="id" property="id" />
	<result column="is_valid" property="isValid" />
	<result column="op_time" property="opTime" />
	<result column="last_ver" property="lastVer" />
	<result column="status" property="status" />
	<result column="type" property="type" />
	<result column="attribute" property="attribute" />
	<result column="create_time" property="createTime" />
		<result column="name" property="name" />
		<result column="code" property="code" />
		<result column="equipment_id" property="equipmentId" />
		<result column="level" property="level" />
		<result column="supply_info" property="supplyInfo" />
		<result column="buy_time" property="buyTime" />
		<result column="memo" property="memo" />
		<result column="check_project_id" property="checkProjectId" />
		<result column="maintain_id" property="maintainId" />
		<result column="fix_id" property="fixId" />
		<result column="position" property="fixId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	id,
	is_valid AS isValid,
	op_time AS opTime,
	last_ver AS lastVer,
	status,
	type,
	attribute,
		position,
	create_time AS createTime,
        name, code, equipment_id AS equipmentId, level, supply_info AS supplyInfo, buy_time AS buyTime, memo, check_project_id AS checkProjectId,
		maintain_id as maintainId,
		fix_id as fixId
    </sql>

</mapper>
