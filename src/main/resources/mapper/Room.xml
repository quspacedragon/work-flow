<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quspacedragon.workflow.mapper.RoomMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.quspacedragon.workflow.entity.Room">
	<result column="id" property="id" />
	<result column="is_valid" property="isValid" />
	<result column="op_time" property="opTime" />
	<result column="last_ver" property="lastVer" />
	<result column="status" property="status" />
	<result column="type" property="type" />
	<result column="attribute" property="attribute" />
		<result column="building_id" property="buildingId" />
		<result column="unit_id" property="unitId" />
		<result column="room_no" property="roomNo" />
		<result column="floor" property="floor" />
		<result column="code" property="code" />
		<result column="area" property="area" />
		<result column="inside_area" property="insideArea" />
		<result column="product_type_Id" property="productTypeId" />
		<result column="memo" property="memo" />
		<result column="business_type" property="businessType" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	id,
	is_valid AS isValid,
	op_time AS opTime,
	last_ver AS lastVer,
	status,
	type,
	attribute,
        building_id AS buildingId, unit_id AS unitId, room_no AS roomNo, floor, code, area, inside_area AS insideArea, product_type_Id AS productTypeId, memo, create_time as createTime,
		business_type as businessType
    </sql>

</mapper>
